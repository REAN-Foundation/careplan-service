{
    "SystemIdentifier": "REAN Careplan Service",
    "Auth" : {
        "Authentication": "Custom",
        "Authorization": "Custom"
    },
    "FileStorage" : {
        "Provider": "AWS-S3"
    },
    "Messaging" : {
        "Provider": "AWS-SNS-SQS",
        "Events": {
                "Enabled": true,
                "RetryPolicy": {
                    "MaxRetries": 2,
                    "BackoffMultiplier": 1.5,
                    "MaxBackoffSeconds": 30
                },
                "DeadLetterQueue": {
                    "Enabled": true,
                    "MaxReceiveCount": 3
                },
                "MessageRetentionPeriod": 1209600,
                "VisibilityTimeout": 300
        }
    },
    "Communication" : {
        "SMS": {
            "Provider": "Twilio"
        },
        "Email": {
            "Provider": "SendGrid"
        }
    },
    "TemporaryFolders": {
        "Upload": "./tmp/resources/uploads/",
        "Download": "./tmp/resources/downloads/",
        "CleanupFolderBeforeMinutes": 10
    },
    "MaxUploadFileSize": 104857600,
    "JwtExpiresIn": 259200
}
